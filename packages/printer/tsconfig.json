{
	"compilerOptions": {
		"lib": [ "DOM", "ES2022" ],
		"moduleResolution": "node",
		"target": "ES2022",
		"module": "ES2022",
		"noEmit": true,
		"checkJs": true,
		"allowJs": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noUncheckedIndexedAccess": true
	}
}
